{
  "name": "collhub-editor",
  "version": "0.2.3",
  "description": "Collhub text editor for CollHub",
  "license": "Apache-2.0",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "module": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "check-types": "tsc --noEmit"
  },
  "peerDependencies": {
    "react": "^18.2.0"
  },
  "dependencies": {
    "@radix-ui/react-popover": "^1.0.7",
    "@tiptap/core": "^2.2.2",
    "@tiptap/extension-color": "^2.2.2",
    "@tiptap/extension-highlight": "^2.2.2",
    "@tiptap/extension-horizontal-rule": "^2.2.2",
    "@tiptap/extension-image": "^2.2.2",
    "@tiptap/extension-link": "^2.2.2",
    "@tiptap/extension-placeholder": "2.2.2",
    "@tiptap/extension-task-item": "^2.2.2",
    "@tiptap/extension-task-list": "^2.2.2",
    "@tiptap/extension-text-style": "^2.2.2",
    "@tiptap/extension-underline": "^2.2.2",
    "@tiptap/pm": "^2.2.2",
    "@tiptap/react": "^2.2.2",
    "@tiptap/starter-kit": "^2.2.2",
    "@tiptap/suggestion": "^2.2.2",
    "@types/node": "20.11.17",
    "@types/react": "18.2.55",
    "@types/react-dom": "18.2.19",
    "@upstash/ratelimit": "^1.0.1",
    "@vercel/analytics": "^1.1.3",
    "@vercel/blob": "^0.21.0",
    "@vercel/kv": "^1.0.1",
    "ai": "^2.2.34",
    "clsx": "^2.1.0",
    "eslint": "8.56.0",
    "eslint-config-next": "14.1.0",
    "eventsource-parser": "^1.1.1",
    "lucide-react": "^0.323.0",
    "next": "14.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-markdown": "^9.0.1",
    "sonner": "^1.4.0",
    "tailwind-merge": "^2.2.1",
    "tippy.js": "^6.3.7",
    "tiptap-markdown": "^0.8.9",
    "typescript": "5.3.3",
    "use-debounce": "^10.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.5",
    "eslint": "^7.32.0",
    "postcss": "^8.4.35",
    "react": "^18.2.0",
    "tailwind-config": "workspace:*",
    "tsconfig": "workspace:*",
    "tsup": "^8.0.2",
    "typescript": "^4.9.4"
  },
  "keywords": [
    "ai",
    "collhub",
    "editor",
    "markdown",
    "nextjs",
    "react"
  ]
}
